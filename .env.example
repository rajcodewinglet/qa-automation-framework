# Environment
ENV=dev

# API URLs
DEV_BASE_URL=https://api-dev.example.com
STG_BASE_URL=https://api-staging.example.com
PROD_BASE_URL=https://api.example.com

# Auth Credentials
DEV_USERNAME=dev_user
DEV_PASSWORD=dev_password
DEV_CLIENT_ID=dev_client_id
DEV_CLIENT_SECRET=dev_client_secret

STG_USERNAME=stg_user
STG_PASSWORD=stg_password
STG_CLIENT_ID=stg_client_id
STG_CLIENT_SECRET=stg_client_secret

PROD_USERNAME=prod_user
PROD_PASSWORD=prod_password
PROD_CLIENT_ID=prod_client_id
PROD_CLIENT_SECRET=prod_client_secret

# Timeouts
DEFAULT_TIMEOUT=30000
REQUEST_TIMEOUT=15000

# Logging
LOG_LEVEL=info